<?xml version = "1.0"?>
<config>
	<modules>
		<!--
			format of tag below is: Namespace_Module
			in our case namespace will be Levismad, and module name is Correios
		-->
		<Levismad_Correios>
			<!--
				Tag active: true will tell magento to enable our module in magento
			-->
			<active>true</active>
			<codePool>local</codePool>
		</Levismad_Correios>
	</modules>
</config>